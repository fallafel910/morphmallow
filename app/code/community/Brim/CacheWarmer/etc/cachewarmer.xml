<config>
    <cachewarmer>
        <sources>
            <homepage>
                <label>Homepage(s)</label>
                <model>brim_cachewarmer/source_homepage</model>
            </homepage>
            <catalog_category>
                <label>Categories</label>
                <model>brim_cachewarmer/source_catalog_category</model>
            </catalog_category>
            <catalog_product>
                <label>Products</label>
                <model>brim_cachewarmer/source_catalog_product</model>
            </catalog_product>
            <catalog_custom>
                <label>Custom Url Rewrites</label>
                <model>brim_cachewarmer/source_catalog_custom</model>
            </catalog_custom>
            <cms>
                <label>CMS Pages</label>
                <model>brim_cachewarmer/source_cms</model>
            </cms>
            <urllist>
                <label>Custom Url List</label>
                <model>brim_cachewarmer/source_urllist</model>
                <args>
                    <!--<basepath></basepath>--> <!-- defaults to {{mage_root}}/var/cachewarmer/ -->
                    <file>url.list</file>
                </args>
            </urllist>
            <sitemap>
                <label>Sitemaps</label>
                <model>brim_cachewarmer/source_sitemap</model>
            </sitemap>
        </sources>
    </cachewarmer>
 </config>